```bash
# ============================================
# PlacementIQ Environment Variables Template
# ============================================
# IMPORTANT: Never commit the actual .env file!
# Copy this file to .env and fill in real values.

# ============================================
# Backend Environment Variables
# ============================================

# MongoDB Connection
# For local development: mongodb://localhost:27017
# For MongoDB Atlas: mongodb+srv://<USERNAME>:<PASSWORD>@<CLUSTER>.mongodb.net/
MONGO_URL=mongodb://localhost:27017

# Database name
DB_NAME=placementiq_db

# CORS Origins - Comma separated list of allowed origins
# For development: *
# For production: https://your-frontend-domain.com,https://www.your-domain.com
CORS_ORIGINS=*

# JWT Secret Key - MUST be a strong random string (minimum 32 characters)
# Generate with: python -c "import secrets; print(secrets.token_urlsafe(32))"
# Or: openssl rand -base64 32
JWT_SECRET_KEY=GENERATE-A-STRONG-RANDOM-SECRET-KEY-HERE-MIN-32-CHARS

# ============================================
# Frontend Environment Variables
# ============================================

# Backend API URL
# For local development: http://localhost:8000
# For production: https://your-backend-url.com
REACT_APP_BACKEND_URL=http://localhost:8000

# Feature flags (set to false in production)
REACT_APP_ENABLE_VISUAL_EDITS=false
ENABLE_HEALTH_CHECK=false

# WebSocket port (for development only)
# WDS_SOCKET_PORT=443

```
